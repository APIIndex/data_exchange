{
  "name": "wallet",
  "version": "3.0.0-alpha.2",
  "publishDate": "2018-5-17",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "DEV": true,
  "MOCK": false,
  "description": "Bottos Wallet",
  "author": {
    "name": "bottos"
  },
  "build": {
    "appId": "bottos.com",
    "asar": true,
    "mac": {
      "category": "Developer Tools",
      "target": [
        "dmg"
      ],
      "icon": "electron_pack/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "electron_pack/icon.ico"
    },
    "linux": {
      "target": [
        "appImage"
      ]
    },
    "directories": {
      "buildResources": "."
    },
    "snap": {
      "useTemplateApp": false
    },
    "extends": null
  },
  "dependencies": {
    "@bottos-project/bottos-crypto-js": "^1.0.5",
    "antd": "^3.2.2",
    "asar": "^0.14.3",
    "bottos-js-crypto": "^1.0.12",
    "buffer": "^5.1.0",
    "crypto-js": "^3.1.9-1",
    "electron-log": "^2.2.14",
    "event-emitter": "^0.3.5",
    "google-protobuf": "^3.5.0",
    "msgpack-lite": "^0.1.26",
    "node-uuid": "^1.4.8",
    "react": "^16.2.0",
    "react-amap": "^1.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.dev.js --hot",
    "build": "webpack --config ./config/webpack.config.prod.js",
    "electron-start": "electron .",
    "mock": "node ./mock/server.js",
    "pack-mac": "electron-builder --platform=darwin",
    "pack-win": "electron-builder --platform=win",
    "pack-linux": "electron-builder --platform=linux",
    "pack-all": "electron-builder --platform=all",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-import": "^1.6.5",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^1.8.3",
    "electron-builder": "^20.11.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "fs-extra": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "less-vars-to-js": "^1.2.1",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react-hot-loader": "^4.1.1",
    "react-transition-group": "^2.3.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "theme": {
    "primary-color": "red"
  }
}
