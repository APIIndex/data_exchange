{
  "name": "wallet",
  "version": "2.2.0",
  "publishDate": "2018-4-26",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "DEV": true,
  "MOCK": false,
  "description": "Bottos Wallet",
  "author": {
    "name": "john",
    "email": "1610712512@qq.com"
  },
  "build": {
    "appId": "bottos.com",
    "asar": true,
    "mac": {
      "category": "Developer Tools",
      "target": [
        "dmg"
      ],
      "icon": "electron_pack/icon.icns"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "electron_pack/icon.ico"
    },
    "linux": {
      "target": [
        "appImage"
      ]
    },
    "directories": {
      "buildResources": "."
    },
    "snap": {
      "useTemplateApp": false
    },
    "extends": null
  },
  "dependencies": {
    "@bottos-project/bottos-crypto-js": "^1.0.5",
    "antd": "^3.2.1",
    "asar": "^0.14.3",
    "buffer": "^5.1.0",
    "crypto-js": "^3.1.9-1",
    "eccrypto": "^1.0.3",
    "electron-log": "^2.2.14",
    "event-emitter": "^0.3.5",
    "node-uuid": "^1.4.8",
    "react": "^16.2.0",
    "react-amap": "^1.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^3.2.0",
    "react-scripts": "1.1.1",
    "recharts": "^1.0.0-beta.10",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron .",
    "electron-package": "electron-packager . bottos --platform=win --out packageApp --arch=x64 --overwrite --ignore=node_modulesls --electron-version=1.8.2",
    "electron-package-all": "electron-packager . bottos --all --out packageAppAll --arch=x64 --overwrite --ignore=node_modulesls --icon=./electron_pack/icon.png --appCopyright=bottos --appVersion=2.0 --buildVersion=2.0  --electron-version=1.8.2",
    "electron-package-win": "electron-packager . bottos --platform=win32 --out packageAppWin --arch=x64 --overwrite --ignore=node_modulesls --icon=./electron_pack/icon.png --appCopyright=bottos --appVersion=2.0 --buildVersion=2.0  --electron-version=1.8.2",
    "mock": "node ./mock/server.js",
    "pack-mac": "electron-builder --platform=darwin",
    "pack-win": "electron-builder --platform=win",
    "pack-linux": "electron-builder --platform=linux",
    "pack-all": "electron-builder --platform=all",
    "postinstall": "electron-builder install-app-deps"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.6.5",
    "electron": "^1.8.2",
    "electron-packager": "^11.0.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "0.0.16",
    "koa-router": "^7.4.0",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.5.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "theme": {
    "primary-color": "red"
  }
}
